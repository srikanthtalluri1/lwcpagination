handleBackToListView() {
        const pageRef = {
            type: 'standard__navItemPage',
            attributes: {
                apiName: 'Waivers_Management'
            }
        };

        this[NavigationMixin.GenerateUrl](pageRef)
            .then(url => {
                console.log('Nav URL:', url);  // check in browser dev tools
                // or temporarily show it in a toast if you want
            });
             this.goToTab();
    }

    goToTab() {
        const pageRef = {
            type: 'standard__navItemPage',
            attributes: {
                apiName: 'Waivers_Management'
            }
        };

        this[NavigationMixin.Navigate](pageRef);
    }
