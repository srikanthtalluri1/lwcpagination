//Srikant added below code to show waive Reason as picklist field 
            getPicklistValues({ objectName: 'AWP_Architecture_Waiver_Form__c', fieldName: 'Waiver_Reason__c' })
            .then(result => {
                this.waiverReasonOptions = result.map(waiverReason => ({ label: waiverReason, value: waiverReason }));
            })
            .catch(error => {
                console.error('Error fetching picklist values:', error);
            });
