@isTest
static void testGetWaiverHistoryRecordsWithAOWValue() {
    AWP_Architecture_Waiver_Form__c waiverRecord = new AWP_Architecture_Waiver_Form__c(
        Name = 'Test Waiver',
        Is_Initial_Creation__c = true
    );
    insert waiverRecord;
    
    AWP_Waiver_Relationship__c waiverRelation = new AWP_Waiver_Relationship__c(
        Name = 'Test Relationship',
        Is_Initial_Creation__c = true
    );
    insert waiverRelation;
    
    Test.startTest();
    
    waiverRelation.Name = 'aOW' + String.valueOf(waiverRecord.Id).substring(0,15);
    update waiverRelation;
    
    List<AWP_WaiverController.WrapperClass> result = AWP_WaiverController.getHistoryRecords(waiverRecord.Id);
    
    Test.stopTest();
    
    System.assert(result != null);
}
